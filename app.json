{
  "expo": {
    "name": "Medium2Freedium",
    "slug": "Medium2Freedium",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "medium2freedium",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "package": "com.bokaboy.medium2freedium",
        "versionCode": 1,
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.bokaboy22.Medium2Freedium"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "73c04da9-1e43-4681-827f-390420f46744"
      }
    },
    "plugins": [
      "expo-router",
      [
        "expo-build-properties",
        {
            "android": {
              "compileSdkVersion": 35,
              "targetSdkVersion": 35,
              "buildToolsVersion": "35.0.0",
              "useLegacyPackaging": true
          },
          "ios": {
            "deploymentTarget": "15.1"
          }
        }
      ],
      [
        "expo-share-intent",
        {
          "androidIntentFilters": ["text/*"]
        }
      ]
    ]
  }
}
